{
  "name": "storj-bridge-gui",
  "version": "0.0.1",
  "description": "A GUI dashboard for Storj Bridge",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Storj/bridge-gui"
  },
  "homepage": "https://app.storj.io/",
  "main": "bin/server.js",
  "scripts": {
    "start": "concurrent --kill-others \"npm run start-prod\" \"npm run start-prod-api\"",
    "start-prod": "better-npm-run start-prod",
    "build": "rm -r static/dist/* && webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
    "postinstall": "webpack --display-error-details --config webpack/prod.config.js",
    "lint": "eslint -c .eslintrc src api",
    "start-dev": "better-npm-run start-dev",
    "watch-client": "better-npm-run watch-client",
    "dev": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev\" \"npm run start-dev-api\"",
    "test": "karma start",
    "test-node": "./node_modules/mocha/bin/mocha $(find api -name '*-test.js') --compilers js:babel-core/register",
    "test-node-watch": "./node_modules/mocha/bin/mocha $(find api -name '*-test.js') --compilers js:babel-core/register --watch"
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080,
        "APIPORT": 3030
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000,
        "APIPORT": 3030
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src"
      }
    }
  },
  "dependencies": {
    "async": "^2.0.0-rc.2",
    "babel": "~5.8.29",
    "babel-plugin-typecheck": "^2.0.0",
    "compression": "^1.6.0",
    "express": "^4.13.3",
    "file-loader": "^0.8.5",
    "storj-bridge-client": "Storj/bridge-client-javascript",
    "multireducer": "^2.0.0",
    "piping": "^0.3.0",
    "pretty-error": "^1.2.0",
    "react": "^0.14.7",
    "react-bootstrap": "^0.28.3",
    "react-dom": "^0.14.1",
    "react-helmet": "^2.2.0",
    "react-loader": "^2.2.0",
    "react-redux": "^4.0.0",
    "react-router": "2.0.0",
    "redux": "^3.0.4",
    "redux-form": "^4.2.2",
    "request": "^2.69.0",
    "serve-favicon": "^2.3.0",
    "url-loader": "^0.5.7",
    "webpack-isomorphic-tools": "^2.2.18"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "~5.8.33",
    "babel-eslint": "^4.1.3",
    "babel-loader": "~5.3.3",
    "babel-plugin-react-transform": "~1.1.1",
    "babel-runtime": "~5.8.29",
    "better-npm-run": "^0.0.4",
    "bootstrap-sass": "3.3.6",
    "bootstrap-sass-loader": "^1.0.9",
    "chai": "^3.3.0",
    "eslint": "1.10.3",
    "eslint-config-airbnb": "0.1.0",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-import": "^0.8.0",
    "eslint-plugin-react": "^3.5.0",
    "clean-webpack-plugin": "^0.1.4",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^0.9.1",
    "karma": "^0.13.10",
    "json-loader": "^0.5.4",
    "karma-cli": "^0.1.1",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.3",
    "phantomjs": "^1.9.18",
    "node-sass": "^3.4.2",
    "phantomjs-polyfill": "0.0.1",
    "react-transform-catch-errors": "^1.0.0",
    "redbox-react": "^1.1.1",
    "redux-devtools": "^3.0.0-beta-3",
    "redux-devtools-dock-monitor": "^1.0.0-beta-3",
    "redux-devtools-log-monitor": "^1.0.0-beta-3",
    "sass-loader": "^3.1.2",
    "sinon": "^1.17.2",
    "strip-loader": "^0.1.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.5.0"
  },
  "engines": {
    "node": "4.1.1"
  }
}
